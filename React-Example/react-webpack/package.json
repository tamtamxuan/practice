{
  "name": "react-webpack",
  "version": "1.0.0",
  "description": "Config Webpack React Flux # Install plugin    `npm install` # Start project   `npm start`",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=react-webpack webpack -p --watch --config webpack.common.js --progress --profile --colors",
    "start": "webpack-dev-server --config webpack.config.js --host 0.0.0.0 --port 9000 --progress --profile --colors --hot --display-cached --content-base dist/",
