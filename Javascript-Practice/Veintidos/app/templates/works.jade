extends ../layouts/default
block variables
  - bodyClass = "works"
block content
  main.animsition
    section.intro__works
      h2.heading__works
        ul.heading__rotate
          li.heading__rotating.active
            span.front creativity
            span.black design
          li.heading__rotating
            span.front excellence
            span.black trend
          li.heading__rotating
            span.front inspiration
            span.black elegance
          li.heading__rotating
            span.front simplicity
            span.black design
      .claim__works
        h3.claim__block
          | Our aspirations lead us to seek
          span excellence
          | with
          span innovative
          | , state-of-the-art projects that are consistent with the latest trends
    .wrapp-works
      section.web__block
        .til-section
          h6 Web
          .line-scroll
        ul.web__preview
          a.animsition-link(href="#")
            li.web__list--mugaku
              .web__name
                h2 Mugako
                .web__name--largue
          a.animsition-link(href="#")
            li.web__list--henry
              .web__name
                h2 Henry saiz
                .web__name--largue
          a.animsition-link(href="#")
            li.web__list--crea
              .web__name
                h2 Ruben crea
                .web__name--largue
          a.animsition-link(href="#")
            li.web__list--room
              .web__name
                h2 Blue room
                .web__name--largue
      section.landing
        .til-section
          h6 Landing
        ul.landing__block
          li.landing__race
            .landing__line
            h3 RACE UNLIMITED
            .image__block
              .landing__image
          li.landing__optimus
            .landing__line
            h3 LG OPTIMUS
            .image__block
              .landing__image
          li.landing__sony
            .landing__line
            h3 LG OPTIMUS
            .image__block
              .landing__image
          li.landing__mahou
            .landing__line
            h3 Ca√±in de Mahou
            .image__block
              .landing__image
          li.landing__consejero
            .landing__line
            h3 El Consejero
            .image__block
              .landing__image
          li.landing__teresa
            .landing__line
            h3 Santa Teresa
            .image__block
              .landing__image
      section.branding
        .til-section
          h6 Branding
        article.branding__mugako
          .logos
            img(src="../images/branding-logo-mugako.jpg")
            img(src="../images/branding-mugako-sello.jpg")
        .carteles
          ul.cover-slider
            li.cover-slider__slide
            li.cover-slider__slide
            li.cover-slider__slide
        .foto-cartel
          img(src="../images/branding-mugako-cartel-medium.jpg")
      section.pages
        .til-section
          h6 Advertising
        ul.pages__block
          li.pages__wikilson
            .pages__image
          li.pages__idealista
            .pages__image
          li.pages__suzuki
            .pages__image
          li.pages__correos
            .pages__image
          li.pages__la-rioja
            .pages__image
          li.pages__chupachups
            .pages__image
          li.pages__groove
            .pages__image
          li.pages__creatividad
            .pages__image
          li.pages__suzuki-uno
            .pages__image
      .footer__back.cl-effect-5
        ul
          li
            a(href='#web')
              span(data-hover='') Web
          li
            a(href='#landing')
              span(data-hover='') Landing
          li
            a(href='#branding')
              span(data-hover='') Branding
          li
            a(href='#pages')
              span(data-hover='') Advertising
